{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "trailingComma": "none",
  "printWidth": 80,
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "tabWidth": 2
      }
    }
  ],
  "endOfLine": "auto"
}
