{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@project/api-core-api": [
        "libs/api/core/api/src/index.ts"
      ],
      "@project/api-core-auth-api": [
        "libs/api/core/auth/api/src/index.ts"
      ],
      "@project/api-core-auth-decorator": [
        "libs/api/core/auth/decorator/src/index.ts"
      ],
      "@project/api-core-auth-guard": [
        "libs/api/core/auth/guard/src/index.ts"
      ],
      "@project/api-core-auth-model": [
        "libs/api/core/auth/model/src/index.ts"
      ],
      "@project/api-core-auth-resource": [
        "libs/api/core/auth/resource/src/index.ts"
      ],
      "@project/api-core-auth-service": [
        "libs/api/core/auth/service/src/index.ts"
      ],
      "@project/api-core-util-crypto": [
        "libs/api/core/util/crypto/src/index.ts"
      ],
      "@project/api-module-accounts-api": [
        "libs/api/module/accounts/api/src/index.ts"
      ],
      "@project/api-module-accounts-model": [
        "libs/api/module/accounts/model/src/index.ts"
      ],
      "@project/api-module-accounts-resource": [
        "libs/api/module/accounts/resource/src/index.ts"
      ],
      "@project/api-module-accounts-service": [
        "libs/api/module/accounts/service/src/index.ts"
      ],
      "@project/api-module-users-api": [
        "libs/api/module/users/api/src/index.ts"
      ],
      "@project/api-module-users-model": [
        "libs/api/module/users/model/src/index.ts"
      ],
      "@project/api-module-users-resource": [
        "libs/api/module/users/resource/src/index.ts"
      ],
      "@project/api-module-users-service": [
        "libs/api/module/users/service/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
